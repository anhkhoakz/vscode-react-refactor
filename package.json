{
        "name": "vscode-react-refactor",
        "displayName": "VSCode React Refactor",
        "description": "Recompose your overgrown JSX without worrying about the given data.",
        "publisher": "anhkhoakz",
        "repository": "https://github.com/anhkhoakz/vscode-react-refactor.git",
        "icon": "assets/images/icon.png",
        "author": "anhkhoakz",
        "keywords": ["react", "refactor", "tools"],
        "license": "ISC",
        "version": "1.2.0",
        "engines": {
                "vscode": "^1.99.0"
        },
        "main": "./dist/extension",
        "scripts": {
                "compile": "bunx tsc -p ./",
                "watch": "bunx tsc -watch -p ./",
                "vscode:prepublish": "webpack --mode production",
                "webpack": "webpack --mode development",
                "webpack-dev": "webpack --mode development --watch",
                "test": "bun test",
                "package": "vsce package",
                "publish": "vsce publish",
                "publish:patch": "vsce publish patch",
                "publish:minor": "vsce publish minor",
                "publish:major": "vsce publish major"
        },
        "activationEvents": ["workspaceContains:package.json"],
        "contributes": {
                "commands": [
                        {
                                "command": "extension.react-refactor.extractToClass",
                                "title": "Extract JSX to Class Component"
                        },
                        {
                                "command": "extension.react-refactor.extractToFunction",
                                "title": "Extract JSX to Functional Component"
                        }
                ],
                "menus": {
                        "explorer/context": []
                },
                "configuration": [
                        {
                                "title": "VSCode React Refactor",
                                "properties": {
                                        "vscodeReactRefactor.functionType": {
                                                "type": "string",
                                                "enum": [
                                                        "function",
                                                        "arrowFunction"
                                                ],
                                                "default": "function",
                                                "description": "The created React Functional Component will be"
                                        },
                                        "vscodeReactRefactor.babelPlugins": {
                                                "type": "string",
                                                "default": "objectRestSpread, classProperties, typescript, jsx",
                                                "description": "List of @babel/parser plugins to use, see: https://babeljs.io/docs/en/babel-parser#plugins"
                                        },
                                        "vscodeReactRefactor.enableDebug": {
                                                "type": "boolean",
                                                "default": false,
                                                "description": "Display parser error messages"
                                        }
                                }
                        }
                ]
        },
        "dependencies": {
                "@babel/core": "^7.28.5",
                "@babel/generator": "^7.28.5",
                "@babel/parser": "^7.28.5",
                "@babel/template": "^7.27.2",
                "@babel/traverse": "^7.28.5",
                "@babel/types": "^7.28.5",
                "lines-and-columns": "^2.0.4",
                "lodash.pickby": "^4.6.0"
        },
        "devDependencies": {
                "@biomejs/biome": "2.3.5",
                "@types/babel__core": "^7.20.5",
                "@types/babel__template": "^7.4.4",
                "@types/babel__traverse": "^7.28.0",
                "@types/bun": "^1.3.2",
                "@types/lodash.pickby": "^4.6.9",
                "@types/node": "^24.10.1",
                "@types/vscode": "^1.99.0",
                "terser": "^5.44.1",
                "ts-loader": "^9.5.4",
                "typescript": "^5.9.3",
                "webpack": "^5.102.1",
                "webpack-cli": "^6.0.1",
                "@vscode/vsce": "^2.26.0"
        },
        "private": true
}
